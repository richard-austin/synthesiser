{
  "name": "synthesiser",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "startProxy": "ng serve --proxy-config src/proxy.conf.js",
    "startProxyNetwork": "ng serve --host 0.0.0.0 --proxy-config src/proxy.conf.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/compiler": "21.1.0-rc.0",
    "@angular/core": "^21.1.0-rc.0",
    "@angular/forms": "^21.1.0-rc.0",
    "@angular/platform-browser": "^21.1.0-rc.0",
    "@angular/router": "^21.1.0-rc.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.0-rc.0",
    "@angular/cli": "^21.1.0-rc.0",
    "@angular/compiler-cli": "^21.1.0-rc.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^25.2.3",
    "baseline-browser-mapping": "^2.9.14",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
